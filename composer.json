{
  "name": "con4gis/tracking-android",
  "description": "Android extension for con4gis-Tracking",
  "keywords": ["con4gis", "tracking", "contao", "c4g", "maps", "gps"],
  "type": "contao-bundle",
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Kuestenschmiede GmbH Software & Design",
      "homepage": "https://kuestenschmiede.de",
      "role": "Operator"
    },
    {
      "name": "con4gis community",
      "homepage": "https://con4gis.org",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@con4gis.org",
    "issues": "https://github.com/Kuestenschmiede/TrackingAndroidBundle/issues"
  },
  "require": {
    "contao/core-bundle": "^4.4",
    "con4gis/tracking": "^3.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\TrackingAndroidBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\TrackingAndroidBundle\\ContaoManager\\Plugin"
  }
}
